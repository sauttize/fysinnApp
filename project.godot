; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="Fysinn: Player Management"
config/tags=PackedStringArray("application", "in_progress")
run/main_scene="res://Elements/welcome_screen.tscn"
config/use_custom_user_dir=true
config/features=PackedStringArray("4.1", "GL Compatibility")
boot_splash/show_image=false
config/icon="res://Icono.png"

[autoload]

GameManager="*res://_assets/Scripts/GameManager.gd"
Utilities="*res://Scenes/Autoloads/Utilities.gd"

[display]

window/size/viewport_width=450
window/size/viewport_height=500
window/subwindows/embed_subwindows=false
window/handheld/orientation=1

[editor]

version_control/plugin_name="GitPlugin"
version_control/autoload_on_startup=true

[gui]

theme/custom_font="res://_assets/Fonts/Rubik/Rubik-Regular.ttf"

[rendering]

renderer/rendering_method="gl_compatibility"
renderer/rendering_method.mobile="gl_compatibility"
